{
  "name": "m2-web-fomo-popup",
  "private": true,
  "engines": {
    "node": "^18.20 || ^20.10 || >=21.0.0"
  },
  "scripts": {
    "dev": "remix dev",
    "build": "remix vite:build",
    "start": "node server.cjs",
    "prisma:generate": "prisma generate",
    "clean": "rimraf ./build ./node_modules/.cache ./prisma/migrations ./.turbo",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "show-vite": "node --input-type=module -e \"console.log(require('fs').readFileSync('vite.config.mjs','utf8').slice(0,800))\"",
    "check-vite-config": "node --input-type=module -e \"import('./vite.config.mjs').then(m=>{const cfg=m.default||m; console.log('HAS DEFAULT:',!!m.default); const names=(cfg?.plugins||[]).map(p=>p?.name); console.log('PLUGINS:', names);}).catch(e=>console.error(e))\"",
    "rebuild-esbuild": "npm rebuild esbuild",
    "print-cwd": "node -e \"console.log('CWD =', process.cwd()); console.log('FILES =', require('fs').readdirSync('.'))\"",
    "remix-version": "node -e \"console.log(require('@remix-run/dev/package.json').version)\"",
    "setup": "npx prisma generate && npx prisma migrate deploy",
    "setup-env": "node scripts/setup-env.js",
    "setup-full": "npm run setup-env && npx prisma generate && npx prisma db push",
    "docker-start": "npm run setup && npm run start",
    "config:link": "shopify app config link",
    "generate": "shopify app generate",
    "deploy": "shopify app deploy",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "prisma": "prisma",
    "graphql-codegen": "graphql-codegen",
    "vite": "vite",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "migrate:status": "prisma migrate status",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "studio": "prisma studio",
    "deploy:plesk": "npm run clean && npm ci && npm run generate && npm run build",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "@remix-run/express": "2.17.0",
    "@remix-run/fs-routes": "2.17.0",
    "@remix-run/node": "2.17.0",
    "@remix-run/react": "2.17.0",
    "@shopify/app-bridge": "^3.7.10",
    "@shopify/app-bridge-react": "^4.2.2",
    "@shopify/polaris": "12.25.0",
    "@shopify/polaris-icons": "^9.3.1",
    "@shopify/polaris-tokens": "^8.0.0",
    "@shopify/shopify-api": "^11.14.1",
    "@shopify/shopify-app-remix": "^3.7.0",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.0",
    "chart.js": "^4.5.0",
    "compression": "^1.7.4",
    "effect": "3.7.2",
    "express": "^4.19.2",
    "fast-check": "3.17.1",
    "isbot": "^5.1.0",
    "lodash": "4.17.21",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.17.0",
    "@remix-run/eslint-config": "2.17.0",
    "@remix-run/route-config": "2.17.0",
    "@remix-run/serve": "2.17.0",
    "@shopify/api-codegen-preset": "^1.1.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.2.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.2.4",
    "prisma": "^6.17.0",
    "typescript": "^5.2.2",
    "vite": "^6.2.2"
  },
  "workspaces": [
    "extensions/*"
  ],
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ],
  "resolutions": {
    "@graphql-tools/url-loader": "8.0.16",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/typescript-operations": "4.5.0",
    "minimatch": "9.0.5",
    "vite": "^6.2.2"
  },
  "overrides": {
    "@graphql-tools/url-loader": "8.0.16",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/typescript-operations": "4.5.0",
    "minimatch": "9.0.5",
    "vite": "^6.2.2"
  },
  "author": "M2 Web Solutions",
  "license": "MIT",
  "description": "Fomo Popup for Shopify - Boost Sales with Social Proof"
}
